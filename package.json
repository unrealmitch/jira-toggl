{
  "name": "toggl2jira",
  "version": "1.7.0",
  "description": "Web extension that sync Toggl Time records to Jira Worklogs",
  "author": "UnrealMitch <unrealmitch@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build:zip": "npm run build && node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch"
  },
  "contributors": [
    "Simon Eldevig <simoneldevig@gmail.com>"
  ],
  "dependencies": {
    "axios": "^0.21.2",
    "vue": "^2.6.14",
    "webextension-polyfill": "^0.3.1",
    "moment": "^2.24.0",
    "vue-material": "^1.0.0-beta-15"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/runtime-corejs3": "^7.14.6",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^3.14.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "ejs": "^2.7.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.14",
    "web-ext-types": "^2.1.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-extension-reloader": "^1.1.4"
  }
}
